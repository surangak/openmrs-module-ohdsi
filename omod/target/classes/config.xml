<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>ohdsi</id>
	<name>OpenMRS OHDSI module Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.ohdsi</package>
	<author>surangak, maurya</author>
	<description>
		OpenMRS OHDSI module
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/ohdsi/update.rdf</updateURL>
	<require_version>1.9.8</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.ohdsi.OpenMRSOHDSImoduleActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.ohdsi.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		ohdsi.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with ohdsi. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

